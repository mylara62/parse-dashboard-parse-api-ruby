<table class="table table-hover table-striped details-table coin-tab-table" id="coin_rank_table">
  <thead>
    <tr>
      <th>Ranking</th>
      <th class="action">User Name</th>
      <th class="action">Amount</th>
    </tr>
  </thead>
  <% unless @ranking_coins.blank? %>
    <% @ranking_coins.each_with_index do |rank,index|%>
    <tbody>
      <tr>
        <td><%= index+1 %></td>
        <td><%= rank_user(rank[:userid])%></td>
        <td><%= rank[:total_amount]%></td>
      </tr>
    </tbody>
    <%end%>
  <%end%>
</table>
