<table class="table table-hover table-striped details-table coin-tab-table" id="receiver_ranking_table">
  <thead>
    <tr>
      <th>Ranking</th>
      <th class="action">User Name</th>
      <th class="action">Amount</th>
    </tr>
  </thead>
  <% unless @receiver_history_ranks.nil? %>
    <% @receiver_history_ranks.each_with_index do |receive_rank,index|%>
    <tbody>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= receive_rank[:receiver_name] %></td>
        <td><%= receive_rank[:total_amount] %></td>
      </tr>
    </tbody>
    <%end%>
  <%end%>
</table>

